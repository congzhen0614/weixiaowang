.double_twelve_page
	// width 100%
	margin 0 auto
	background-color #fcf4d4
	img
		display inline-block
		width 100%
		// height 100%
	.twelve_page_top
		position relative
		width 100%
		background-size 100% 100%
		.top_main_title
			margin-left auto
			margin-right auto
		.top_right_corner
			position absolute
			right 0
			bottom 0
	.kinder_garten_list
		margin-top 0.2rem
		width 100%
		.kinder_garten_item
			width 100%
			margin-top 0.8rem
			&:first-child
				margin-top 0
	.primary_package_list
		width 100%
		.primary_package_item
			width 100%
			margin-top 0.8rem
	.twelve_special_part
		margin-top 1.5rem
		.special_main_part
			width 100%
			padding 1.03rem 0.5rem 1.18rem
			display flex
			background-color #f26f48
			overflow hidden
			.special_main_left
				border 0.05rem solid #fff
				border-radius 0.05rem
				box-shadow 0.1rem 0.1rem 0.1rem 0 rgba(0, 0, 0, 0.2)
				background-size contain
				background-repeat no-repeat
				background-position center center
			.special_main_right
				flex 1
				margin-left 0.4rem
				.special_desc_top
					margin-top 0.1rem
				.special_buy_btn
					margin-top 0.2rem
					text-align right
				.special_right_line
					margin-top 0.25rem
					line-height 0
				.special_want_more
					margin-top 0.23rem
	.twelve_nav_list
		margin-top 1.6rem
		display flex
		padding 0 0.8rem
		.nav_left_list
			flex 1
			.nav_item
				margin-top 0.5rem
				margin-left auto
				margin-right auto
				width 1.8rem
				&:first-child
					margin-top 0
		.nav_right_list
			flex 1
			.nav_item
				margin-top 0.5rem
				margin-left auto
				margin-right auto
				width 1.8rem
				&:first-child
					margin-top 0
		.nav_center_line
			width 0.26rem
			margin-top 0.3rem
	.twelve_page_bottom
		margin-top 2rem